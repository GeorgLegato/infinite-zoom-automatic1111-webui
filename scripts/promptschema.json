{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "prompts": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "array",
                        "items": [
                            {
                                "oneOf": [
                                    {
                                        "type": "integer",
                                        "minimum": 0
                                    },
                                    {
                                        "type": "string"
                                    }
                                ]
                            },
                            {
                                "type": "string"
                            }
                        ],
                        "minItems": 2,
                        "maxItems": 2,
                        "uniqueItems": true
                    },
                    "minItems": 1
                },
                "headers": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "minItems": 2
                }
            },
            "required": [
                "data",
                "headers"
            ]
        },
        "negPrompt": {
            "type": "string"
        }
    },
    "required": [
        "prompts",
        "negPrompt"
    ]
}